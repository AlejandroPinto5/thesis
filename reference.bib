@article{Martin1997RecentSolids,
    title = {{\href{https://doi.org/10.1016/S0038-1098(96)00719-3}{Recent developments in the theory of electric polarization in solids}}},
journal = {Solid State Communications},
volume = {102},
number = {2},
pages = {121-126},
year = {1997},
note = {Highlights in Condensed Matter Physics and Materials Science},
issn = {0038-1098},
doi = {https://doi.org/10.1016/S0038-1098(96)00719-3},
author = {Richard M. Martin and Gerardo Ortiz},
abstract = {Polarization, the dipole moment per unit volume of a material, is one of the basic quantities in physics which is essential to the theory of dielectrics, effective charges in lattice dynamics, piezoelectricity, ferroelectricity and other phenomena. For extended matter such as crystals, however, a theory of polarization formulated directly in terms of the quantum mechanical wavefunction of the electrons has only recently been derived. Quantum mechanics is the problem: because the electrons do not belong to an individual unit cell of a crystal and their density is continuous, there is no way to uniquely “cut” the density and derive the dipole moment of a cell. The new approach consists in relating the change in polarization to the integrated bulk current flowing through the unit cell and quantum mechanics provides the solution: unique expressions for the integrated current in an insulator can be found from a geometric Berry's phase involving integrals over the phases of the electronic wavefunctions. This method has been used to calculate many properties such as effective charges and ferroelectric moments, and also to discover new phenomena such as divergences of effective charges at magnetic phase transitions.}
}
